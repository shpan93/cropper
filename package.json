{
  "name": "durian",
  "version": "0.0.0",
  "private": true,
  "main": "start.js",
  "scripts": {
    "build": "webpack --config webpack.config.production.js",
    "dev": "node webpack-dev-server.js & PORT=3001 NODE_ENV=local nodemon start.js -e js,jsx",
    "dev[windows]": "node_modules/.bin/npm-run-all --parallel dev-wepback[windows] dev-nodemon[windows]",
    "dev-wepback[windows]": "node webpack-dev-server.js",
    "dev-nodemon[windows]": "set PORT=3001&&set NODE_ENV=local&&nodemon start.js -e js,jsx",
    "lint": "eslint ./*.js ./**/*.js",
    "start": "NODE_ENV=production node start.js",
    "start[windows]": "set NODE_ENV=production&&node start.js",
    "test": "NODE_ENV=test mocha --recursive --require babel-core/register --timeout 60000 --slow 10",
    "test[windows]": "set NODE_ENV=test&&mocha --recursive --require babel-core/register --timeout 60000 --slow 10",
    "test-debug": "NODE_ENV=test mocha --recursive --require babel-core/register --timeout 60000 --slow 10 --debug-brk",
    "test-debug[windows]": "set NODE_ENV=test&&mocha --recursive --require babel-core/register --timeout 60000 --slow 10 --debug-brk"
  },
  "pre-push": [
    "test",
    "lint"
  ],
